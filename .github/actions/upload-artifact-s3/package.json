{
  "name": "upload-artifact",
  "version": "2.0.1",
  "description": "Upload a build artifact that can be used by subsequent workflow steps",
  "main": "dist/index.js",
  "scripts": {
    "all": "npm run format && npm run build && npm run lint && npm run release",
    "build": "tsc",
    "release": "ncc build src/upload-artifact.ts",
    "check-all": "concurrently \"npm:format-check\" \"npm:lint\" \"npm:test\" \"npm:build\"",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint **/*.ts",
    "test": "jest --testTimeout 10000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/upload-artifact.git"
  },
  "keywords": [
    "Actions",
    "GitHub",
    "Artifacts",
    "Upload"
  ],
  "author": "GitHub",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/actions/upload-artifact/issues"
  },
  "homepage": "https://github.com/actions/upload-artifact#readme",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.0.0",
    "@actions/glob": "^0.1.0",
    "@actions/io": "^1.0.2",
    "aws-sdk": "^2.908.0",
    "mime": "^2.5.2"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/node": "^12.12.6",
    "@typescript-eslint/parser": "^4.33.0",
    "@zeit/ncc": "^0.22.1",
    "concurrently": "^5.1.0",
    "eslint": "^7.26.0",
    "eslint-plugin-github": "^4.1.3",
    "eslint-plugin-jest": "^23.8.2",
    "glob": "^7.1.6",
    "jest": "^26.6.3",
    "jest-circus": "^26.1.0",
    "prettier": "^2.0.5",
    "ts-jest": "^26.0.0",
    "typescript": "^3.8.3"
  }
}
